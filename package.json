  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "build": "yarn run clean && yarn run build:client && yarn run build:server",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "heroku-postbuild": "yarn run build",
    "init": "ts-node src/server/level/init-level.ts",
    "lint:client": "tslint \"src/client/**/*.ts\"",
    "lint:server": "tslint \"src/server/**/*.ts\"",
    "lint": "tslint \"src/**/*.ts\"",
    "postbuild": "yarn run init",
    "start": "cross-env NODE_ENV=production node index.js",
    "test:client": "cross-env NODE_ENV=test karma start",
    "test:server": "cross-env NODE_ENV=test jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "test": "yarn run test:server && yarn run test:client"
  },
